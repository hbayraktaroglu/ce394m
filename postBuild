jupyter contrib nbextension install --user
jupyter nbextension enable --py widgetsnbextension
jupyter nbextension enable python-markdown/main

# Notebooks w/ extensions that auto-run code must be "trusted" to work the first time
jupyter trust notebooks/index.ipynb
jupyter trust notebooks/00-stiffness_method.ipynb
jupyter trust notebooks/01-shape_functions.ipynb
jupyter trust notebooks/02-elastic_bar_linear_fem.ipynb
jupyter trust notebooks/03-fdm_seepage_dam.ipynb